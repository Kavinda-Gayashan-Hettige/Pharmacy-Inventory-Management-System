	<query language="SQL"><![CDATA[SELECT 
    customer_id,
    customer_name,
    medicine_name,
    qty,
    sale_price,
    (qty * sale_price) AS total_amount,
    sale_date,
    CAST(MONTH(sale_date) AS CHAR) AS sale_month,
    CAST(YEAR(sale_date) AS CHAR) AS sale_year
FROM customers
WHERE YEAR(sale_date) = $P{ReportYear}
  AND MONTH(sale_date) = $P{ReportMonth};
]]></query>
